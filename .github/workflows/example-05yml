name: example05
on: [push]
jobs: 
  Test1:
    runs-on: ubuntu-latest
    steps: 
      - run: echo "step1"
  Test2:
    runs-on: ubuntu-latest
    needs: test1
    steps:
      - run: echo "step2"
  Test3:
    runs-on: ubuntu-latest
    needs: Test1
    steps:
      - run: echo "test3"
  Test4:
    runs-on: ubuntu-latest
    needs: ["Test2","Test3"]
    steps:
      - run: echo "test4"